1 TYPELIB "versioninfo.rc"

1 VERSIONINFO
 FILEVERSION @IN_VERSION_MAJOR@, @IN_VERSION_MINOR@, @IN_VERSION_PATCH@, 0
 PRODUCTVERSION @IN_VERSION_MAJOR@, @IN_VERSION_MINOR@, @IN_VERSION_PATCH@, 0
 FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x4L
 FILETYPE 0x2L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904e4"
        BEGIN
            VALUE "CompanyName", "@IN_COMPANYNAME@"
            VALUE "FileDescription", "@IN_FILEDESCRIPTION@"
            VALUE "FileVersion","@IN_VERSION_MAJOR@, @IN_VERSION_MINOR@, @IN_VERSION_PATCH@, 0"
            VALUE "InternalName", "@IN_INTERNALNAME@"
            VALUE "LegalCopyright", "@IN_LEGALCOPYRIGHT@"
            VALUE "OriginalFilename", "@IN_ORIGINALFILENAME@"
            VALUE "ProductName", "@IN_PRODUCTNAME@"
            VALUE "ProductVersion","@IN_VERSION_MAJOR@, @IN_VERSION_MINOR@, @IN_VERSION_PATCH@"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1252
    END
END
